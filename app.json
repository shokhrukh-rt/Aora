{
	"expo": {
		"name": "Aora",
		"slug": "aora",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/logo.png",
		"scheme": "aora",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "",
				"backgroundColor": "#ffffff"
			},
			"package": "com.zevon.aora"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": ""
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true
		}
	}
}
